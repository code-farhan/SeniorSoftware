extends project

block content
	h1.page-header Demo
	div.placeholder
	div.row.placeholders
		div.col-xs-12.col-sm-6.col-md-4.placeholder
			form.form-horizontal(action="post",id="changeset-form")
				div.input-group.se-input-group
					label Project changeset URL base
					input.form-control.se-input(type="text",name="url",placeholder="Project changeset URL base",disabled,value="http://trac.edgewall.org/changeset/")
				div.input-group.se-input-group.row
					div.col-xs-6.col-sm-6.col-md-6(style="float:left")
						label Start Page
						input.form-control.se-input(type="number",name="start",disabled,value="7560")
					div.col-xs-6.col-sm-6.col-md-6(style="float:right")
						label End Page
						input.form-control.se-input(type="number",name="end",disabled,value="7580")
				div.input-group.se-input-group
					label Regex expressionfor author 
					input.form-control.se-input(type="text",name="author",disabled)
				div.input-group.se-input-group
					label Regex expressionfor for ticket
					input.form-control.se-input(type="text",name="ticket",disabled)
				div.input-group.se-input-group
					label Regex expressionfor for version
					input.form-control.se-input(type="text",name="version",disabled)
				div.input-group.se-input-group
					label Regex expressionfor for timestamp
					input.form-control.se-input(type="text",name="timestamp",disabled)
				div.input-group.se-input-group
					label Regex expressionfor for message
					input.form-control.se-input(type="text",name="message",disabled)
				div.input-group.se-input-group
					label Regex expressionfor for file
					input.form-control.se-input(type="text",name="file",disabled)
				div.input-group.se-input-group
					label Regex expressionfor for type
					input.form-control.se-input(type="text",name="type",disabled)
				div.input-group.se-input-group
					input.btn.se-btn(type="submit" ,value="Changeset Crawl")
		div.col-xs-12.col-sm-4.col-md-4.placeholder
			form.form-horizontal(action="post",id="ticket-form")
				div.input-group.se-input-group
					label Project ticket URL base
					input.form-control.se-input(type="text",name="url",placeholder="Project ticket URL base",disabled,value="http://trac.edgewall.org/ticket/")
